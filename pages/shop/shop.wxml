<!--pages/shop/shop.wxml-->
<text>pages/shop/shop.wxml</text>

<view wx:for="{{arr}}"  wx:for-item="item">
  {{item}}
</view>


<view wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="row">
  <view wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="col" wx:if="{{clo <= row}}">
    {{col}} * {{row}} = {{col*row}}
  </view>
</view>